{
  "name": "fpc",
  "description": "Functional piping & composition",
  "version": "2.4.0",
  "author": "Emiliano Bovetti",
  "license": "GPL-3.0",
  "keywords": [
    "functional",
    "composition",
    "compose",
    "combine",
    "fluent",
    "pipeline",
    "piping",
    "pipe",
    "fp",
    "typescript"
  ],
  "main": "./src/index",
  "types": "./src/index.d.ts",
  "umd:main": "./dist/fpc.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/emilianobovetti/fpc.git"
  },
  "bugs": {
    "url": "https://github.com/emilianobovetti/fpc/issues"
  },
  "release": {
    "branch": "master",
    "prepare": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "dist/*.js",
          "docs/*.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@semantic-release/git": "^7.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.9.0",
    "eslint-loader": "^2.1.1",
    "jsverify": "^0.8.3",
    "lodash.isequal": "^4.5.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "semantic-release": "^15.12.4",
    "should": "^13.2.3",
    "should-sinon": "^0.0.6",
    "sinon": "^7.1.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {}
}
